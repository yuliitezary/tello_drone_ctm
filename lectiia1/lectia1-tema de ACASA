from djitellopy import Tello

tello = Tello()
tello.connect()
print(tello.get_battery())

tello.takeoff()

tello.move_forward(60)
tello.flip_forward()
tello.flip_back()
tello.rotate_clockwise(360)
tello.move_left(60)
tello.rotate_clockwise(360)
tello.move_back(60)
tello.move_up(35)
tello.rotate_clockwise(360)
tello.rotate_counter_clockwise(360)
tello.move_right(60)
tello.flip_right()
tello.flip_left()
tello.move_down(35)
tello.rotate_clockwise(360)
tello.move_right(60)
tello.move_back(20)

tello.land()
